    "@sa/color": "workspace:*",
    "@sa/utils": "workspace:^",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.13",
    "element-plus": "^2.9.1",
    "pinia": "^2.3.0",
    "sass": "^1.82.0",
    "vue": "^3.5.13"
